<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="buttonModel"
            type="ir.amozkade.advancedAsisstiveTouche.mvvm.assistiveButton.models.ButtonModelInPanel" />
    </data>
        <ir.amozkade.advancedAsisstiveTouche.helper.customviews.CircularTextViewWithBorder
